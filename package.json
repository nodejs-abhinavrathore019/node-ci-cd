{
  "name": "nodejs-ci-cd",
  "version": "0.0.0-dev",
  "description": "ci/cd implementation using github actions, circle-ci and jenkins",
  "main": "dist/app.js",
  "scripts": {
    "install-1": "npm install --prefix 1-github",
    "install-2": "npm install --prefix 2-circle-ci",
    "install-3": "npm install --prefix 3-jenkins",
    "install-all": "npm run install-1 && npm run install-2 && npm run install-3",
    "dev-1": "npm run dev --prefix 1-github",
    "dev-2": "npm run dev --prefix 2-circle-ci",
    "dev-3": "npm run dev --prefix 3-jenkins",
    "prod-1": "npm run prod --prefix 1-github",
    "prod-2": "npm run prod --prefix 2-circle-ci",
    "prod-3": "npm run prod --prefix 3-jenkins"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "express",
    "nodejs"
  ],
  "author": "deltacap019",
  "license": "ISC",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "11.1.0",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/github": "9.2.3",
    "@semantic-release/release-notes-generator": "12.1.0",
    "semantic-release": "22.0.7"
  },
  "releaseTag": "1.3.4"
}
